<?xml version="1.0" encoding="utf-8"?>
<!-- NOTE:
    - this file is made automaticaly by (project-root)/tools/layout.py:
        adjustment  -> $ADJUSTMENT_INFO
        key height  -> $KEY_HEIGHT_INFO
        orientation -> $ORIENTATION_INFO
    - actual keymap is defined in KeymapController.kt
-->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="17%p"
    android:horizontalGap="0dp"
    android:verticalGap="0dp"
    android:keyHeight="@dimen/$KEY_HEIGHT_VALUE"
